{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/fetch",
        "dest": "/main.py"
      },
      {
        "src": "/download",
        "dest": "/main.py"
      }
    ]
  }
  